{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "YourConnectionStringHere"
  },
  "JwtSettings": {
    "SecretKey": "YourSuperSecretKeyHere12345YourSecretKeyHere12345", // Replace with a strong secret key
    "Issuer": "YourAppName", // Replace app's name
    "Audience": "YourAppNameAudience", // Replace audience name
    "ExpiryInMinutes": 60 // Token expiration time in minutes
  }
}